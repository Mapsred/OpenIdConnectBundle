{
    "name": "maps_red/openidconnectbundle",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.1.0",
        "symfony/framework-bundle": "~3.0|^4.0"
    },
    "require-dev": {
        "jumbojett/openid-connect-php": "^0.3.0",
        "symfony/console": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/security-bundle": "~3.0|^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Maps_red\\OpenIDConnectBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maps_red\\OpenIDConnectBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
